{
  "name": "develitesse-nuxt",
  "version": "2.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.5",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && pnpm simple-git-hooks install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck --no-emit",
    "taze:up": "taze major -I",
    "test": "vitest",
    "cleanup": "nuxt cleanup",
    "release:bump": "changelogen --bump",
    "release": "npx changelogen --release --push"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@iconify-json/heroicons": "^1.1.20",
    "@iconify-json/svg-spinners": "^1.1.2",
    "@nuxt/devtools": "^1.1.5",
    "@nuxt/eslint": "0.3.0-beta.7",
    "@nuxt/image": "^1.4.0",
    "@nuxt/test-utils": "^3.12.0",
    "@nuxtjs/color-mode": "^3.3.3",
    "@pinia/nuxt": "^0.5.1",
    "@unocss/nuxt": "^0.58.9",
    "@vueuse/nuxt": "^10.9.0",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "lint-staged": "^15.2.2",
    "magic-regexp": "^0.8.0",
    "nuxt": "^3.11.1",
    "nuxt-radash": "^1.0.0",
    "nuxt-time": "^0.1.3",
    "pinia": "^2.1.7",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.13.3",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.7"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit $1",
    "pre-commit": "npx lint-staged"
  },
  "overrides": {
    "vue": "latest"
  }
}